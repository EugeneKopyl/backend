{
  "name": "backend",
  "version": "1.0.0",
  "description": "MERN",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "npm run lint",
    "start": "nodemon --exec ts-node src/server.ts",
    "debug": "npm i ts-node-dev --save-dev",
    "server:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint:fixi": "eslint --cache --ext .ts . --fix",
    "lint": "eslint --cache --ext .ts .",
    "lint:fix": "npm run lint:ts:fix && npm run lint:prettier:fix",
    "lint:ts": "npx eslint \"./src/**\"",
    "lint:ts:fix": "npx eslint \"./src/**/*\" --fix",
    "lint:prettier": "prettier --list-different \"**/*.*\"",
    "lint:prettier:fix": "prettier --write \"**/*.*\""
  },
  "author": "Eugen.Kopyl@gmail.com",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "log4js": "^6.9.1",
    "memorystore": "^1.6.7",
    "mongoose": "^7.3.3",
    "nodemon": "^3.0.1",
    "passport": "^0.5.3",
    "passport-forcedotcom": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
